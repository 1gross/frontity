{
  "name": "typescript-basic",
  "version": "0.2.12",
  "private": true,
  "description": "The minimum setup for Frontity using TypeScript",
  "scripts": {
    "dev": "frontity dev",
    "dev:inspect": "node -r ts-node/register/transpile-only --inspect ./node_modules/frontity/src/commands.ts dev",
    "build": "frontity build",
    "build:inspect": "node -r ts-node/register/transpile-only --inspect ./node_modules/.bin/frontity build",
    "serve": "frontity serve",
    "serve:inspect": "node -r ts-node/register/transpile-only --inspect ./node_modules/.bin/frontity serve"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@frontity/core": "^1.1.3",
    "@frontity/extension-example-1": "^1.1.12",
    "@frontity/extension-example-2": "^1.2.12",
    "@frontity/tiny-router": "^1.0.12",
    "frontity": "^1.2.2"
  }
}
